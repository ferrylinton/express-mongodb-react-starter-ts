{
	"name": "express-mongodb-react-starter-ts",
	"version": "1.0.0",
	"description": "Express + Mongodb + React starter (with typescript)",
	"main": "./dist/server.js",
	"scripts": {
		"clean": "rimraf dist",
		"start": "cd dist && cross-env NODE_ENV=production node --enable-source-maps ./server.js",
		"build-backend": "webpack --config ./webpack.config.server.js --mode=production",
		"build-frontend": "cross-env NODE_ENV=build vite build",
		"build": "npm run clean && concurrently 'npm:build-*'",
		"dev": "cross-env NODE_ENV=development nodemon",
		"test": "jest --runInBand --detectOpenHandles --forceExit",
		"lint": "eslint --fix .",
		"format": "prettier . --write",
		"prepare": "husky",
		"faker": "cross-env NODE_ENV=development tsx --tsconfig tsconfig.server.json ./src/server/db/faker.ts"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,js,html,json}": [
			"npm run lint",
			"npm run format"
		]
	},
	"keywords": [
		"express",
		"react",
		"mongodb",
		"typscript"
	],
	"author": "ferrylinton",
	"license": "ISC",
	"dependencies": {
		"@radix-ui/colors": "^3.0.0",
		"@radix-ui/react-alert-dialog": "^1.1.2",
		"@radix-ui/react-checkbox": "^1.1.2",
		"@radix-ui/react-collapsible": "^1.1.1",
		"@radix-ui/react-dropdown-menu": "^2.1.2",
		"@radix-ui/react-icons": "^1.3.0",
		"@radix-ui/react-popover": "^1.1.2",
		"@radix-ui/react-select": "^2.1.2",
		"@radix-ui/react-switch": "^1.1.1",
		"@radix-ui/react-toast": "^1.2.2",
		"@radix-ui/react-tooltip": "^1.1.3",
		"animate.css": "^4.1.1",
		"axios": "^1.7.7",
		"bcryptjs": "^2.4.3",
		"clsx": "^2.1.1",
		"cookie-parser": "^1.4.7",
		"dotenv": "^16.4.5",
		"express": "^4.21.1",
		"express-favicon": "^2.0.4",
		"js-cookie": "^2.2.1",
		"jsonwebtoken": "^9.0.2",
		"mongodb": "^6.9.0",
		"nodemailer": "^6.9.16",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-intl": "^6.7.0",
		"react-router-dom": "^6.26.2",
		"uuid": "^10.0.0",
		"winston": "^3.15.0",
		"zod": "^3.23.8",
		"zustand": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@faker-js/faker": "^9.2.0",
		"@types/bcryptjs": "^2.4.6",
		"@types/cookie-parser": "^1.4.7",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.13",
		"@types/js-cookie": "^3.0.6",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/node": "^22.7.7",
		"@types/nodemailer": "^6.4.16",
		"@types/react": "^18.0.38",
		"@types/react-dom": "^18.2.16",
		"@types/supertest": "^6.0.2",
		"@types/uuid": "^10.0.0",
		"@vitejs/plugin-react": "^4.2.0",
		"autoprefixer": "^10.4.20",
		"concurrently": "^9.0.1",
		"copy-webpack-plugin": "^12.0.2",
		"cross-env": "^7.0.3",
		"eslint": "^9.13.0",
		"eslint-plugin-jest": "^28.8.3",
		"globals": "^15.11.0",
		"husky": "^9.1.6",
		"jest": "^29.7.0",
		"lint-staged": "^15.2.10",
		"nodemon": "^3.1.7",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"rimraf": "^6.0.1",
		"supertest": "^7.0.0",
		"tailwindcss": "^3.4.14",
		"ts-jest": "^29.2.5",
		"ts-loader": "^9.5.1",
		"tsx": "^4.19.1",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.10.0",
		"typescript-plugin-css-modules": "^5.1.0",
		"vite": "^5.0.2",
		"vite-express": "^0.19.0",
		"vite-plugin-virtual-html": "^1.2.2",
		"webpack": "^5.95.0",
		"webpack-cli": "^5.1.4"
	}
}
